/*
//c program to calculate the sum of odd and even numbers between 1 to n
#include<stdio.h>

int main(){
int num, oddSum=0, evenSum=0;
int i;
printf("This program calculate the sum of number between 1 and N\nEnter N:");
scanf("%i", &num);
for (i=1;i<=num;i++)
 if((i%2)==0)
 evenSum+=i;
else
    oddSum+=i;
printf("Odd sum= %i\nEven sum= %i", oddSum,evenSum);
return 0;
}
