/*
//c program compute the sum of digits in a given number
#include<stdio.h>
int main(){
int num;
int numSum=0;
printf("Enter the integer number: ");
scanf("%i", &num);
while(num !=0){
    numSum+=(num%10);
    num/=10;
}
    printf("The sum of the integer numbers digit= %i", numSum);

return 0;
